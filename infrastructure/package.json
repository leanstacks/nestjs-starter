{
  "name": "nestjs-starter-infrastructure",
  "version": "1.1.0-alpha.1",
  "description": "AWS CDK infrastructure as code for the NestJS Starter application.",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf cdk.out coverage dist",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:all": "cdk deploy --all",
    "destroy": "cdk destroy",
    "destroy:all": "cdk destroy --all",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap"
  },
  "dependencies": {
    "aws-cdk-lib": "2.238.0",
    "constructs": "10.5.1",
    "dotenv": "17.3.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "25.3.0",
    "aws-cdk": "2.1106.1",
    "jest": "30.2.0",
    "rimraf": "6.1.3",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
}
